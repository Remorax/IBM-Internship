<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:seals="http://www.seals-project.eu/ontologies/SEALSMetadata.owl#"
    xmlns:dc="http://purl.org/dc/terms/" > 
    

	<!-- MATCHING PROBLEM -->
	
	<!-- HP and MP-->

	<rdf:Description rdf:about="http://www.seals-project.eu/alignment/PhenotypeSuiteTest">
		<rdf:type rdf:resource="http://www.seals-project.eu/ontologies/SEALSMetadata.owl#Suite"/>
		<seals:hasSuiteItem rdf:resource="http://www.seals-project.eu/alignment/PhenotypeSuiteTest#hp-mp"/>		
	</rdf:Description>

	

	<rdf:Description rdf:about="http://www.seals-project.eu/alignment/PhenotypeSuiteTest#hp-mp">
		<rdf:type rdf:resource="http://www.seals-project.eu/ontologies/SEALSMetadata.owl#SuiteItem"/>
		<seals:hasDataItem rdf:resource="http://www.seals-project.eu/alignment/PhenotypeSuiteTest#hp"/>
		<seals:hasDataItem rdf:resource="http://www.seals-project.eu/alignment/PhenotypeSuiteTest#mp"/>
		<seals:hasDataItem rdf:resource="http://www.seals-project.eu/alignment/PhenotypeSuiteTest#hp-mp-baseline"/>
		<dc:identifier>hp-mp</dc:identifier>
	</rdf:Description>



	<!-- ONTOLOGIES -->

	<!-- HP Human Phenotype ontology -->
	<rdf:Description rdf:about="http://www.seals-project.eu/alignment/PhenotypeSuiteTest#hp">
		<rdf:type rdf:resource="http://www.seals-project.eu/ontologies/SEALSMetadata.owl#DataItem"/>
		<seals:hasComponentType>source</seals:hasComponentType>
		<seals:isLocatedAt>hp.owl</seals:isLocatedAt>
		<dc:identifier>hp</dc:identifier>
	</rdf:Description>

	

	<!-- MP Mammalian Phenotype ontology -->
	<rdf:Description rdf:about="http://www.seals-project.eu/alignment/PhenotypeSuiteTest#mp">
		<rdf:type rdf:resource="http://www.seals-project.eu/ontologies/SEALSMetadata.owl#DataItem"/>
		<seals:hasComponentType>target</seals:hasComponentType>
		<seals:isLocatedAt>mp.owl</seals:isLocatedAt>
		<dc:identifier>mp</dc:identifier>
	</rdf:Description>
	
	
	

	<!-- Baseline reference: based on BioPortal -->	
	<rdf:Description rdf:about="http://www.seals-project.eu/alignment/PhenotypeSuiteTest#hp-mp-baseline">
		<rdf:type rdf:resource="http://www.seals-project.eu/ontologies/SEALSMetadata.owl#DataItem"/>
		<seals:hasComponentType>reference</seals:hasComponentType>
		<seals:isLocatedAt>HP_MP.rdf</seals:isLocatedAt>
		<dc:identifier>hp-mp-baseline</dc:identifier>
	</rdf:Description>


</rdf:RDF>
