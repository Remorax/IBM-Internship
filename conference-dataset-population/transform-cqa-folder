#!/bin/sh


CQAInFolder=`pwd`/CQAs/

CQAOutFolder=`pwd`/CQAs-per-onto2

listOfCQAs=`ls $CQAInFolder`


rm -rf $CQAOutFolder
mkdir $CQAOutFolder
for cqa in $listOfCQAs
  do
  listOfOntos=`ls $CQAInFolder/$cqa`

  for onto in $listOfOntos  
   do	
	#echo $onto
	
	ontoEmpty=`basename $onto .sparql`
mkdir $CQAOutFolder/$ontoEmpty
    	cp $CQAInFolder/$cqa/$ontoEmpty.sparql $CQAOutFolder/$ontoEmpty/$cqa.sparql
	
    done
done
