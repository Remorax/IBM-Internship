              <edoal:AttributeOccurenceRestriction>
                <edoal:onAttribute>
                  <edoal:Relation rdf:about="&cmt;rejectedBy" />
                </edoal:onAttribute>
                <edoal:comparator rdf:resource="&edoal;greater-than" />
