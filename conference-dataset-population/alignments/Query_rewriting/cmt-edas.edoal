        <relation>Subsumes</relation>
      </Cell>
    </map>


    <!--For AcceptedPaper, both members of the edoal:or are individually correct-->
    <map>
      <Cell rdf:about="AcceptedPaper">
        <entity1>
          <edoal:Class>
            <edoal:or rdf:parseType="Collection">
              <edoal:AttributeDomainRestriction>
                <edoal:onAttribute>
                  <edoal:Relation rdf:about="&cmt;hasDecision" />
                </edoal:onAttribute>
                <edoal:class>
                  <edoal:Class rdf:about="&cmt;Acceptance" />
