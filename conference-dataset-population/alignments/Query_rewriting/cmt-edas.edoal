            <edoal:or rdf:parseType="Collection">
              <edoal:Class rdf:about="&edas;ConferenceChair" />
              <edoal:Class rdf:about="&edas;SessionChair" />
            </edoal:or>
          </edoal:Class>
        </entity2>
        <measure rdf:datatype="&xsd;float">1.0</measure>
        <relation>Subsumes</relation>
      </Cell>
    </map>
    
