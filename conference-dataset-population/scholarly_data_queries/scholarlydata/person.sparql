PREFIX person: <https://w3id.org/scholarlydata/person/>
PREFIX conf: <https://w3id.org/scholarlydata/ontology/conference-ontology.owl#>

SELECT DISTINCT ?person ?name ?fname ?lname
WHERE{
  ?person a conf:Person.
  ?person conf:name ?name.
  ?person conf:givenName ?fname.
 ?person conf:familyName ?lname.
}
order by ?person
limit {{numPerson}}
offset {{offsetPerson}}
